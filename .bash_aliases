## Directory Navigation ##
alias l='ls -CF'
alias lla='ls -la'
alias la='ls -A'
alias ll='ls -l'
alias lh='ll -h'
alias p='pushd'
alias o='popd'
alias cll='clear;ll'
alias whois='whois -H'
## Log Management ##
alias tf='tail -f'
alias t60='tail -n 60'
alias h60='head -n 60'
## Git Shortcuts ##
alias gci='git ci -am'
alias gpo='git push origin'
## Graphical Environments ##
alias emacs='emacs -nw'
alias gvim="gvim -reverse 2>/dev/null"
