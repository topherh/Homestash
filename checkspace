#!/bin/bash


UWPLONE2=`ssh uwplone2 df -h '/dev/sda1' | awk '{print $5}'`
UWPLONE3=`ssh uwplone3 df -h '/dev/mapper/datavg-data' | awk '{print $4}'`
EMERG=`ssh em1 df -h '/dev/xvda' | awk '{print $5}'`
INFO1=`ssh info1 df -h '/dev/sda7' | awk '{print $5}'`

echo -ne "CMS Devel : \t";echo $UWPLONE2| sed 's/Use% //'
echo -ne "CMS Prod  : \t";echo $UWPLONE3| sed 's/Avail //'
echo -ne "UW EM1    : \t";echo $EMERG| sed 's/Use% //'
echo -ne "UW INFO1  : \t";echo $INFO1| sed 's/Use% //'
